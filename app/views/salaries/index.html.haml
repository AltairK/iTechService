.page-header
  %h1= auto_title

.table_filter.navbar.container
  .navbar-inner
    = form_tag salaries_path, method: 'get', id: 'search_form', class: 'navbar-search form-search', remote: true do
      .input-append
        = text_field_tag :user_q, params[:user_q], id: 'salaries_search_input', class: 'search-query input-medium',
            placeholder: Salary.human_attribute_name(:user)
        %span.add-on.btn.clear_search_input
          %i.icon-remove

#resource_table= render 'table'
